{
  "name": "scottie-crump-dot-com",
  "version": "3.0.2",
  "private": true,
  "scripts": {
    "start": "next-remote-watch ./data",
    "dev": "next dev",
    "build": "next build",
    "test": "vitest run --reporter=json --outputFile=metrics_results/vitest-results.json",
    "test:watch": "vitest",
    "test:interative": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "cov-report": "open coverage/index.html",
    "playwright": "npx playwright test",
    "playwright:interactive": "npx playwright test --ui",
    "playwright:interactive:local": "npx playwright test --ui --config=playwright.local.config.ts",
    "playwright:update-browsers": "npx playwright install",
    "playwright:report": "npx playwright show-report",
    "allure-report": "npx allure serve allure-results",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts",
    "prepare": "husky install",
    "send-metrics": "node scripts/send-metrics.mjs"
  },
  "dependencies": {
    "@datadog/browser-rum": "^5.27.0",
    "@influxdata/influxdb3-client": "^0.11.0",
    "@tailwindcss/forms": "^0.5.8",
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.5",
    "esbuild": "^0.23.1",
    "gray-matter": "^4.0.2",
    "image-size": "1.0.0",
    "mdx-bundler": "^10.0.3",
    "next": "^14.2.23",
    "next-themes": "^0.0.14",
    "postcss": "^8.4.41",
    "preact": "^10.23.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "reading-time": "1.3.0",
    "rehype-katex": "^6.0.0",
    "rehype-prism-plus": "^2.0.0",
    "remark-autolink-headings": "^7.0.1",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "remark-slug": "^7.0.1",
    "sharp": "0.32.6",
    "tailwindcss": "^3.4.10",
    "unist-util-visit": "^4.0.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "11.1.0",
    "@playwright/test": "^1.50.1",
    "@svgr/webpack": "^6.3.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/k6": "^0.53.2",
    "@types/node": "^20",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.9",
    "allure-commandline": "^2.30.0",
    "allure-playwright": "^3.0.0-beta.12",
    "allure-vitest": "^3.0.0-beta.12",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.8",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-testing-library": "^6.3.0",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "file-loader": "^6.0.0",
    "globals": "^15.9.0",
    "globby": "11.0.3",
    "husky": "^6.0.0",
    "inquirer": "^8.1.1",
    "jsdom": "^25.0.0",
    "k6": "^0.0.0",
    "lint-staged": "^11.0.0",
    "next-remote-watch": "^1.0.0",
    "prettier": "2.2.1",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.9"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.17.0"
  }
}
